{
  "name": "@kintisoft/sdk",
  "version": "0.1.11",
  "description": "Official JavaScript/TypeScript SDK for KintiSoft APIs",
  "type": "module",       
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "kintisoft",
    "sdk",
    "api",
    "multitenant"
  ],
  "author": "KintiSoft",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "devDependencies": {
    "typescript": "^5.0.0"
  }
}
